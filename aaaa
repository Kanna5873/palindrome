#include <iostream>
#include<bits/stdc++.h>
#include<string>
using namespace std;
void print(string str,int s,int e,int n)
{
    string temp;
    int j=0;
    int l=0;
    int max=0;
    for(int i=s;i<=e;i++)
    {
        temp.push_back(str[i]);
        //cout<<temp<<endl;
    }
    int m= temp.size();
    int flag[m]={0};
    //cout<<temp<<endl;
    for(int i=0;i<m;i++)
    {
        int count=0;
        if(flag[i]!=1)
        {
            for(int j=i;j<m;j++)
            {
                if(temp[i]==temp[j])
                {
                    count++;
                    flag[j]=1;
                }
            }
            //int l=0;
            if(count==1)
            {
               //int l=0;
               l++;
            }
            cout<<l<<endl;
            if(l==m)
            {
                cout<<temp<<endl;
            }
             //cout<<"occurence of "<<temp[i]<<" "<<count<<endl;
        }
    }
}
void sub(string str,int n)
{
    for(int s=0;s<n;s++)
    {
        for(int e=s;e<n;e++)
        {
            print(str,s,e,n);
            
        }
    }
}
int main()
{
    string str;
    cout<<"enter the string ";
    getline(cin,str);
    int n=str.size();
    sub(str,n);
    return 0;
}
